CREATE DATABASE IncomeDB;
USE IncomeDB;

CREATE TABLE IncomeData (
    id INT AUTO_INCREMENT PRIMARY KEY,
    monthly_value DECIMAL(10, 2) NOT NULL
);

INSERT INTO IncomeData (monthly_value) VALUES (3500.00);
INSERT INTO IncomeData (monthly_value) VALUES (4200.00);
INSERT INTO IncomeData (monthly_value) VALUES (4900.00);
INSERT INTO IncomeData (monthly_value) VALUES (5200.00);

SELECT
    id,
    monthly_value,
    CASE
        WHEN monthly_value <= 4000 THEN 'Low Income'
        WHEN monthly_value <= 5000 THEN 'Avg Income'
        ELSE 'High Income'
    END AS income_level
FROM IncomeData;
